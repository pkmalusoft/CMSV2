                        <label class="headinglabel required"> Qty</label>

                        <input type="number" class="form-control text-right " id="Qty" name="Qty" onchange="calculatevalue1()" />
                    </div>
                    <div class="col-md-1">
                        <label class="headinglabel required "> Rate</label>
                        <input type="text" class="form-control text-right" id="Rate" name="Rate" onchange="calculatevalue1()" />
                    </div>
                    <div class="col-md-1">
                        <label class="headinglabel required "> Amount</label>
                        <input type="text" class="form-control text-right" readonly id="Amount" name="Amount" onchange="calculatevalue1()" />
                    </div>
                    <div class="col-md-1">
                        <label class="headinglabel "> Tax %</label>
                        <input type="number" class="form-control" id="Taxpercent" name="Taxpercent" onchange="calculatevalue1()" />
                    </div>
